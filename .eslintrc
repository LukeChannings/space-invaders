{
  "extends": [
    "standard",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      },
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "quotes": [2, "backtick", "avoid-escape"],
    "comma-dangle": [2, "only-multiline"],
    "indent": [2, 2],
    "comma-style": 0,
    "react-jsx-in-scope": 0
  }
}

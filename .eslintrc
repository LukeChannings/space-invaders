{
  "extends": [
    "standard"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      },
  },
  "plugins": [
    "immutable"
  ],
  "rules": {
    "strict": 0,
    "comma-dangle": [2, "only-multiline"],
    "indent": [2, 2],
    "no-unused-vars": [2, { "varsIgnorePattern": "h" }],
    "comma-style": 0,
    "new-parens": 0,
    "quotes": [1, "backtick"],
    "immutable/no-let": 2,
    "immutable/no-this": 2,
    "immutable/no-mutation": 2
  }
}
